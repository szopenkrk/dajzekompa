    "scripts": {
        "build": "webpack",
        "build:client": "webpack --config-name client",
        "build:server": "webpack --config-name server",
        "clean": "rm -rf build/",
        "deploy:build": "NODE_ENV=production npm run build && npm run package && docker image build -t eu.gcr.io/dajzekompa/dajzekompa . && docker image push eu.gcr.io/dajzekompa/dajzekompa",
        "deploy:apply": "kubectl apply -f ./k8.yaml",
        "deploy:update": "kubectl set image deployment/dajzekompa dajzekompa=eu.gcr.io/dajzekompa/dajzekompa",
        "db:migrate": "knex migrate:latest -- --envfile env-local.env",
        "db:rollback": "knex migrate:rollback -- --envfile env-local.env",
        "dev": "nodemon -e ts,tsx,html,css --exec \"npm run start\"",
        "lint": "eslint 'src/**/*.{js,ts,tsx}'",
        "package": "cd build && tar -zcf ../artifact.tar.gz . && cd ..",
        "start": "npm run build && node -r dotenv/config build/app.js dotenv_config_path=env-local.env",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
